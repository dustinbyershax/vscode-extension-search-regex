{
  "name": "vscode-extension-search-regex",
  "displayName": "search-multiple-words-in-file",
  "description": "An extension for searching a codebase for files containing ALL words",
  "publisher": "DustinByers",
  "version": "0.0.2",
  "engines": {
    "vscode": "^1.34.0"
  },
  "categories": [
    "Other"
  ],
  "activationEvents": [
    "onCommand:extension.findAllStrings"
  ],
  "main": "./out/extension.js",
  "contributes": {
    "commands": [
      {
        "command": "extension.findAllStrings",
        "title": "Find files containing all strings"
      }
    ]
  },
  "scripts": {
    "vscode:prepublish": "npm run compile",
    "compile": "tsc -p ./",
    "lint": "eslint . --ext .ts,.tsx",
    "watch": "tsc -watch -p ./"
  },
  "devDependencies": {
    "@types/node": "^16.11.7",
    "@types/vscode": "^1.34.0",
    "@typescript-eslint/eslint-plugin": "^5.19.0",
    "@typescript-eslint/parser": "^5.19.0",
    "eslint": "^8.13.0",
    "typescript": "^4.7.2"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/dustinbyershax/vscode-extension-search-regex.git"
  },
  "author": "dustinbyershax",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/dustinbyershax/vscode-extension-search-regex/issues"
  },
  "homepage": "https://github.com/dustinbyershax/vscode-extension-search-regex#readme"
}
